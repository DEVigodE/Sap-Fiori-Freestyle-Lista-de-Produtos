{
  "_version": "1.48.0",
  "sap.app": {
    "id": "br.com.nel.freestyle.ibm.report.products",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.4",
      "toolsId": "cc4e54f6-877c-44f4-ac82-ee3a2eb8208b"
    },
    "dataSources": {
      "ZIBM_SB_PRODUCTS_VAN": {
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZIBM_SB_PRODUCTS_VAN',Version='0001')/$value/",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "localService/mainService/ZIBM_SB_PRODUCTS_VAN.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZIBM_SB_PRODUCTS/",
        "type": "OData",
        "settings": {
          "annotations": [
            "ZIBM_SB_PRODUCTS_VAN"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "ZIBMRPRODUCTS-display": {
          "semanticObject": "ZIBMRPRODUCTS",
          "action": "display",
          "title": "{{ZIBMRPRODUCTS-display.flpTitle}}",
          "subTitle": "{{ZIBMRPRODUCTS-display.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "br.com.nel.freestyle.ibm.report.products.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "br.com.nel.freestyle.ibm.report.products.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteLista",
          "pattern": ":?query:",
          "target": [
            "TargetLista"
          ]
        }
      ],
      "targets": {
        "TargetLista": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Lista",
          "viewName": "Lista"
        }
      }
    },
    "rootView": {
      "viewName": "br.com.nel.freestyle.ibm.report.products.view.Objetos",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
